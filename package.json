{
  "name": "meteor-pg",
  "version": "2.0.0-beta",
  "description": "This module makes live PostgreSQL select queries work with the Meteor publish/subscribe mechanism. Relies heavily on the module numtel/pg-live-select.",
  "main": "lib/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "babel --presets es2015 -d lib/ src/",
    "build-watch": "babel --watch --presets es2015 -d lib/ src/",
    "prepublish": "npm run build",
    "postinstall": "npm run build"
  },
  "keywords": [
    "meteor",
    "postgresql",
    "psql",
    "pg",
    "meteor-pg",
    "pg-live-select",
    "reactive"
  ],
  "author": "Richard Faasen",
  "repository": "https://github.com/richie765/meteor-pg-live.git",
  "license": "MIT",
  "dependencies": {
    "pg-live-select": "richie765/pg-live-select",
    "pg-native": "^1.10.0",
    "pg-promise": "^5.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-preset-es2015": "^6.9.0"
  }
}
