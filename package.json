{
  "name": "@richie765/meteor-pg",
  "version": "2.0.0-beta",
  "description": "This module makes live PostgreSQL select queries work with the Meteor publish/subscribe mechanism. Relies heavily on the module numtel/pg-live-select.",
  "main": "dist/index.js",
  "dependencies": {
    "pg-live-select": "richie765/pg-live-select",
    "pg-native": "^1.10.0",
    "pg-promise": "^5.0.3",
    "babel-cli": "^6.11.4",
    "babel-preset-es2015": "^6.9.0"
  },
  "devDependencies": {
    "babel-cli": "6.14.0",
    "babel-plugin-syntax-async-functions": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-stage-3": "6.11.0"
  },
  "scripts": {
    "build": "babel src -d dist --source-maps inline",
    "build-watch": "babel --watch src -d dist --source-maps inline",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Richie765/meteor-pg.git"
  },
  "keywords": [
    "meteor",
    "postgresql",
    "psql",
    "pg",
    "meteor-pg",
    "pg-live-select",
    "reactive"
  ],
  "author": "Richard",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Richie765/meteor-pg/issues"
  },
  "homepage": "https://github.com/Richie765/meteor-pg#readme"
}
